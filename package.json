{
  "name": "stylus-extra",
  "version": "1.0.0",
  "description": "Wrap stylus to ease recursive file detection and watching",
  "main": "stylus-extra.js",
  "bin": "./stylus-extra.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "deploy": "npm publish"
