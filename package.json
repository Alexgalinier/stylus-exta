    "stylus",
    "watcher"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexgalinier/stylus-exta.git"
  },
  "bugs": {
    "url": "https://github.com/Alexgalinier/stylus-exta/issues"
  },
  "author": "Alexgalinier",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "chalk": "^2.3.2",
    "commander": "^2.15.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "sane": "^2.4.1",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^22.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js"
    ]
